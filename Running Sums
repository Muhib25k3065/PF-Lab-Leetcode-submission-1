#include <stdio.h>

int main()
{
	int i, j, nums[5];
	int runningSum[5];
	int sum = 0;
	
	for (i = 0; i < 5; i++)
	{
		printf("Enter number %d: ", i+1);
		scanf("%d", &nums[i]);
	}
	
	for (i = 0; i < 5; i++)
	{
		for (j = 0; j < i+1; j++)
		{
			sum = sum + nums[j];
		}
		runningSum[i] = sum;
		sum = 0;
	}

	printf("\n");
	
	printf("The normal nums are: ");
	
	for (i = 0; i < 5; i++)
	{
		printf("%d ", nums[i]);
	}

	printf("\nThe running sums are: ");
	
	for (i = 0; i < 5; i++)
	{
		printf("%d ", runningSum[i]);
	}
	
	return 0;
}
